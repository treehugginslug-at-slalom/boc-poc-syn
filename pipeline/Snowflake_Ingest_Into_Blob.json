{
	"name": "Snowflake_Ingest_Into_Blob",
	"properties": {
		"activities": [
			{
				"name": "Copy Data From Snowflake To ADLS",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Set As Of Date",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SnowflakeV2Source",
						"exportSettings": {
							"type": "SnowflakeExportCopyCommand"
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings",
							"copyBehavior": "PreserveHierarchy"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "Snowflake_Raw_Source",
						"type": "DatasetReference",
						"parameters": {
							"SchemaName": {
								"value": "@pipeline().parameters.SchemaName",
								"type": "Expression"
							},
							"TableName": {
								"value": "@pipeline().parameters.TableName",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "BOC_Azure_Blob_Storage",
						"type": "DatasetReference",
						"parameters": {
							"FileName": "@concat(pipeline().parameters.TableName,'.parquet')"
						}
					}
				]
			},
			{
				"name": "Set As Of Date",
				"type": "SetVariable",
				"dependsOn": [],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "As_Of_Date",
					"value": {
						"value": "@convertTimeZone(utcnow(),'UTC','Pacific Standard Time')",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Load ADLS to Snowflake Raw",
				"type": "Script",
				"state": "Inactive",
				"onInactiveMarkAs": "Succeeded",
				"dependsOn": [
					{
						"activity": "Copy Data From Snowflake To ADLS",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"linkedServiceName": {
					"referenceName": "Snowflake_BOC_POC",
					"type": "LinkedServiceReference"
				},
				"typeProperties": {
					"scripts": [
						{
							"type": "Query",
							"text": {
								"value": "@concat('call BOC.RAW.SP_LOAD_AZURE_TO_SNOWFLAKE(',\n'''',pipeline().parameters.TableName, '''',\n');'\n)\n ",
								"type": "Expression"
							}
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
			}
		],
		"parameters": {
			"SchemaName": {
				"type": "string",
				"defaultValue": "RAW"
			},
			"TableName": {
				"type": "string",
				"defaultValue": "IBS_DIM_CUSTOMER"
			},
			"SystemName": {
				"type": "string",
				"defaultValue": "IBS"
			},
			"VersionNumber": {
				"type": "string",
				"defaultValue": "Version1"
			}
		},
		"variables": {
			"As_Of_Date": {
				"type": "String"
			}
		},
		"annotations": []
	}
}